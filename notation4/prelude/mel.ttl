mel:propertyChainOf a rdf:Property;
  rdfs:label "propertyChainOf";
  rdfs:comment "The property that determines the sequence of object or datatype properties that build a chain of steps from first step to last.";
  rdfs:isDefinedBy mel:;
  rdfs:domain rdf:Property;
  rdfs:range rdf:List
  .

mel:hasIsA a owl:AnnotationProperty;
  rdfs:label "hasIsA";
  rdfs:comment "The property that determines the 'is a' property for a class.";
  rdfs:isDefinedBy mel:;
  rdfs:domain rdfs:Class;
  rdfs:range rdf:Property
  .
