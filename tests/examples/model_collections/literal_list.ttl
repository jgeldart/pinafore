            rdf:type rdf:List;
            rdf:first "Hello"^^xsd:string;
            rdf:rest [
              rdf:type rdf:List;
              rdf:first "Goodbye"@en;
              rdf:rest rdf:nil;
            ]
          ]
        ]
      ]
    ]
  ].

